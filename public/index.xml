    <item>
      <title>Payment</title>
      <link>http://localhost:1313/hd-fund/bricks/payment/</link>
      <pubDate>Mon, 01 Jan 0001 00:00:00 +0000</pubDate>
      <guid>http://localhost:1313/hd-fund/bricks/payment/</guid>
      <description>&lt;h1 id=&#34;choose-your-payment-method&#34;&gt;Choose your payment method&lt;/h1&gt;&#xA;&lt;p&gt;You will be paying &amp;lsquo;Usecue&amp;rsquo; and you will see your order number as a reference. You will have to pay &lt;span id=&#34;paymenttotal&#34;&gt;&lt;/span&gt;.&lt;/p&gt;&#xA;&lt;div id=&#34;smart-button-container&#34; style=&#34;max-width: 350px;&#34;&gt;&#xD;&#xA;&#xD;&#xA;    &lt;input type=&#34;hidden&#34; name=&#34;descriptionInput&#34; id=&#34;description&#34;&gt;&#xD;&#xA;    &lt;input type=&#34;hidden&#34; name=&#34;amountInput&#34; id=&#34;amount&#34;&gt;&#xD;&#xA;    &lt;input type=&#34;hidden&#34; name=&#34;invoiceid&#34; id=&#34;invoiceid&#34;&gt;&#xD;&#xA;&#xD;&#xA;    &lt;script&gt;&#xD;&#xA;        window.onload = function() {&#xD;&#xA;            var amount = (getCartTotal() + getAddonTotal()).toFixed(2); &#xD;&#xA;            var ordernumber = localStorage.getItem(&#39;ordernumber&#39;);&#xD;&#xA;            &#xD;&#xA;            document.getElementById(&#39;description&#39;).value = ordernumber;&#xD;&#xA;            document.getElementById(&#39;invoiceid&#39;).value = ordernumber;&#xD;&#xA;            document.getElementById(&#39;amount&#39;).value = amount;&#xD;&#xA;        }&#xD;&#xA;    &lt;/script&gt;&#xD;&#xA;    &lt;div style=&#34;text-align: center; margin-top: 0.625rem;&#34; id=&#34;paypal-button-container&#34;&gt;&lt;/div&gt;&#xD;&#xA;&lt;/div&gt;&#xD;&#xA;&#xD;&#xA;&lt;script src=&#34;https://www.paypal.com/sdk/js?client-id=ARLAOALKgJkpvAynfrxup8sP6NiLzBTpe6leWyLs7QJHnUMsCt8tKKVFWJzLg4zP0jky8IlPddUX2YCt&amp;currency=USD&amp;components=buttons&#34;&gt;&lt;/script&gt;&#xD;&#xA;&lt;script&gt;&#xD;&#xA;&#xD;&#xA;function initPayPalButton() {&#xD;&#xA;&#xD;&#xA;var amount = document.querySelector(&#39;#smart-button-container #amount&#39;);&#xD;&#xA;var description = document.querySelector(&#39;#smart-button-container #description&#39;);&#xD;&#xA;var invoiceid = document.querySelector(&#39;#smart-button-container #invoiceid&#39;);&#xD;&#xA;&#xD;&#xA;var purchase_units = [];&#xD;&#xA;purchase_units[0] = {};&#xD;&#xA;purchase_units[0].amount = {};&#xD;&#xA;&#xD;&#xA;function validate(event) {&#xD;&#xA;  return event.value.length &gt; 0;&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;paypal.Buttons({&#xD;&#xA;  style: {&#xD;&#xA;    color: &#39;blue&#39;,&#xD;&#xA;    shape: &#39;pill&#39;,&#xD;&#xA;    label: &#39;pay&#39;,&#xD;&#xA;    layout: &#39;vertical&#39;,&#xD;&#xA;    &#xD;&#xA;  },&#xD;&#xA;&#xD;&#xA;  onInit: function (data, actions) {&#xD;&#xA;    &#xD;&#xA;  },&#xD;&#xA;&#xD;&#xA;  onClick: function () {&#xD;&#xA;    &#xD;&#xA;    purchase_units[0].description = description.value;&#xD;&#xA;    purchase_units[0].amount.value = amount.value&#xD;&#xA;&#xD;&#xA;    if(invoiceid.value !== &#39;&#39;) {&#xD;&#xA;      purchase_units[0].invoice_id = invoiceid.value;&#xD;&#xA;    }&#xD;&#xA;  },&#xD;&#xA;&#xD;&#xA;  createOrder: function (data, actions) {&#xD;&#xA;    return actions.order.create({&#xD;&#xA;      purchase_units: purchase_units,&#xD;&#xA;    });&#xD;&#xA;  },&#xD;&#xA;&#xD;&#xA;  onApprove: function (data, actions) {&#xD;&#xA;    return actions.order.capture().then(function (details) {&#xD;&#xA;      alert(&#39;Transaction completed by &#39; + details.payer.name.given_name + &#39;!&#39;);&#xD;&#xA;    });&#xD;&#xA;  },&#xD;&#xA;&#xD;&#xA;  onError: function (err) {&#xD;&#xA;    console.log(err);&#xD;&#xA;  }&#xD;&#xA;}).render(&#39;#paypal-button-container&#39;);&#xD;&#xA;}&#xD;&#xA;initPayPalButton();&#xD;&#xA;&lt;/script&gt;</description>
    </item>
